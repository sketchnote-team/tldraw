{
  "name": "sn-wb-tldraw",
  "version": "1.79.0",
  "description": "A tiny little drawing app (editor)",
  "author": "@steveruizok",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tldraw/tldraw.git"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "canvas",
    "zoom",
    "drawing",
    "whiteboard"
  ],
  "files": [
    "dist/**/*"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "scripts": {
    "start": "yarn lask -d",
    "build": "yarn lask",
    "start:packages": "yarn start",
    "start:core": "yarn start",
    "build:core": "yarn build",
    "build:packages": "yarn build",
    "lint": "eslint src/ --ext .ts,.tsx",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "jest",
    "test:ci": "jest --ci --runInBand --updateSnapshot",
    "test:watch": "jest --watchAll",
    "docs": "typedoc"
  },
  "peerDependencies": {
    "react": "^17.0",
    "react-dom": "^17.0"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^0.1.5",
    "@radix-ui/react-checkbox": "^0.1.4",
    "@radix-ui/react-context-menu": "^0.1.4",
    "@radix-ui/react-dropdown-menu": "^0.1.4",
    "@radix-ui/react-icons": "^1.0.3",
    "@radix-ui/react-radio-group": "^0.1.4",
    "@radix-ui/react-tooltip": "^0.1.6",
    "@stitches/react": "^1.2.6",
    "@tldraw/core": "^1.9.1",
    "@tldraw/intersect": "^1.7.1",
    "@tldraw/vec": "^1.7.0",
    "embed-video": "^2.0.4",
    "idb-keyval": "^6.1.0",
    "perfect-freehand": "^1.0.16",
    "react-contenteditable": "^3.3.6",
    "react-hotkey-hook": "^1.0.2",
    "react-hotkeys-hook": "^3.4.4",
    "sanitize-html": "^2.7.1",
    "sn-wb-tldraw-core": "^1.7.0",
    "tslib": "^2.3.1",
    "zustand": "^3.6.9"
  },
  "devDependencies": {
    "@swc-node/jest": "^1.4.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@tldraw/core": "*",
    "@tldraw/intersect": "*",
    "@tldraw/vec": "*",
    "@types/node": "^17.0.14",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "lask": "^0.0.29",
    "mobx": "^6.3.8",
    "react": ">=16.8",
    "react-dom": "^16.8 || ^17.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/../../setupTests.ts"
    ],
    "transform": {
      "^.+\\.(tsx|jsx|ts|js|mjs)?$": "@swc-node/jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironment": "jsdom",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "moduleNameMapper": {
      "@tldraw/tldraw": "<rootDir>/src",
      "\\~(.*)": "<rootDir>/src/$1"
    }
  },
  "bugs": {
    "url": "https://github.com/tldraw/tldraw/issues"
  },
  "homepage": "https://github.com/tldraw/tldraw#readme"
}
